.bar-title{
  margin-top:18px;
  display:flex;
  width:100%;
  padding:10px 15px;
}
.bar-title .bar-label{
  line-height:35px;
  padding-left:10px;
  flex:1;
}
.bar-title .bar-more{
  padding-right:10px;
}
.button{
  height:35px;
  line-height:35px;
  border-radius:35px 35px 35px 35px;
  border:1px solid #000;
  font-size:16px;
  min-width:80px;
}

.prolist{
  width:100%;
}
.prolist .product{
  width:50%;
  float:left;
  box-sizing:border-box;
  padding:10px 10px 10px 30px;
  margin-bottom:10px;
}
.prolist .product view{
  margin-left:10px;
  font-size:12px;
}
.prolist .product image{
  width:120px;
  height:120px;

}